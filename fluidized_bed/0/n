FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      n;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1.0;

boundaryField
{
    inletWalls
    {
        type            zeroGradient;
    }
    outletWalls
    {
      type              zeroGradient;
    }
    fixedWalls
    {
        type            zeroGradient;
    }
}

